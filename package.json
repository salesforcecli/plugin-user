{
  "name": "@salesforce/plugin-user",
  "description": "Commands to interact with Users and Permission Sets",
  "version": "2.3.2",
  "author": "Salesforce",
  "main": "lib/index.js",
  "bugs": "https://github.com/forcedotcom/cli/issues",
  "dependencies": {
    "@oclif/core": "^2.1.0",
    "@salesforce/core": "^3.33.1",
    "@salesforce/kit": "^1.8.5",
    "@salesforce/sf-plugins-core": "^2.1.1",
    "@salesforce/ts-types": "^1.7.2",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/plugin-command-snapshot": "^3.3.6",
    "@salesforce/cli-plugins-testkit": "^3.2.21",
    "@salesforce/dev-config": "^3.1.0",
    "@salesforce/dev-scripts": "^3.1.1",
    "@salesforce/plugin-command-reference": "^2.3.1",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "^1.4.4",
    "@swc/core": "^1.3.27",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.50.0",
    "chai": "^4.3.7",
    "chai-each": "^0.0.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-salesforce": "^1.1.1",
    "eslint-config-salesforce-license": "^0.1.6",
    "eslint-config-salesforce-typescript": "^1.1.1",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^39.8.0",
    "eslint-plugin-sf-plugin": "^1.8.1",
    "husky": "^7.0.4",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "oclif": "^3.6.2",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.0",
    "shx": "0.3.4",
    "sinon": "10.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.9.5",
    "wireit": "^0.9.3"
  },
  "config": {},
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/salesforcecli/plugin-user",
  "keywords": [
    "force",
    "salesforce",
    "sfdx",
    "salesforcedx",
    "sfdx-plugin"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-command-snapshot",
      "@salesforce/plugin-command-reference"
    ],
    "topics": {
      "force": {
        "external": true,
        "subtopics": {
          "user": {
            "description": "Perform user-related admin tasks.",
            "subtopics": {
              "permset": {
                "description": "Interact with permission sets assigned to a user."
              },
              "permsetlicense": {
                "description": "Interact with permission set licenses assigned to a user."
              },
              "password": {
                "description": "Generate and set passwords for users."
              }
            }
          }
        }
      },
      "org": {
        "description": "commands that perform user-related admin tasks",
        "subtopics": {
          "assign": {
            "description": "Used to assign permissions to a user"
          },
          "create": {
            "description": "Used to create a user"
          },
          "display": {
            "description": "Used to display a user"
          },
          "list": {
            "description": "Used to list users"
          },
          "generate": {
            "description": "Generate commands for a user"
          }
        }
      }
    }
  },
  "repository": "salesforcecli/plugin-user",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "sf-prepack",
    "prepare": "sf-install",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "test:command-reference": "wireit",
    "test:compile": "wireit",
    "test:deprecation-policy": "wireit",
    "test:json-schema": "wireit",
    "test:nuts": "nyc mocha \"**/*.nut.ts\" --slow 4500 --timeout 600000 --parallel --jobs 10",
    "test:only": "wireit",
    "version": "oclif readme"
  },
  "publishConfig": {
    "access": "public"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "compile",
        "lint"
      ]
    },
    "compile": {
      "command": "tsc -p . --pretty --incremental",
      "files": [
        "src/**/*.ts",
        "tsconfig.json",
        "messages/**"
      ],
      "output": [
        "lib/**",
        "*.tsbuildinfo"
      ],
      "clean": "if-file-deleted"
    },
    "format": {
      "command": "prettier --write \"+(src|test|schemas)/**/*.+(ts|js|json)|command-snapshot.json\""
    },
    "lint": {
      "command": "eslint src test --color --cache --cache-location .eslintcache",
      "files": [
        "src/**/*.ts",
        "test/**/*.ts",
        "messages/**",
        ".eslintignore",
        ".eslintrc.js"
      ],
      "output": []
    },
    "test:compile": {
      "command": "tsc -p \"./test\" --pretty",
      "files": [
        "test/**/*.ts",
        "tsconfig.json",
        "test/tsconfig.json"
      ],
      "output": []
    },
    "test": {
      "dependencies": [
        "test:compile",
        "test:only",
        "test:command-reference",
        "test:deprecation-policy",
        "lint",
        "test:json-schema"
      ]
    },
    "test:only": {
      "command": "nyc mocha \"test/**/*.test.ts\"",
      "files": [
        "test/**/*.ts",
        "src/**/*.ts",
        "tsconfig.json",
        "test/tsconfig.json"
      ],
      "output": []
    },
    "test:command-reference": {
      "command": "\"./bin/dev\" commandreference:generate --erroronwarnings",
      "files": [
        "src/**/*.ts",
        "messages/**"
      ],
      "output": [
        "tmp/root"
      ]
    },
    "test:deprecation-policy": {
      "command": "\"./bin/dev\" snapshot:compare",
      "files": [
        "src/**/*.ts"
      ],
      "output": [],
      "dependencies": [
        "compile"
      ]
    },
    "test:json-schema": {
      "command": "\"./bin/dev\" schema:compare",
      "files": [
        "src/**/*.ts",
        "schemas"
      ],
      "output": []
    }
  }
}
